<% provide(:title, 'New Photo') %>
<h1>New Photo</h1>

<div class="row">
  <div class="col-xs-12 col-md-6">
    <%= form_for @photo,:html => {:multipart => true}, url: shoot_photos_path(shoot_id: @photo.shoot_id)  do |f| %>
<p>
	<%= f.label :photo %>
      <%= f.file_field :photo %>
</p>

      <%= f.submit "Submit New Photo", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>